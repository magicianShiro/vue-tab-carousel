{
  "name": "vue-tab-carousel",
  "version": "1.3.0",
  "description": "一个集成了tab,carousel,loadmore的移动端vue组件",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magicianShiro/vue-tab-carousel.git"
  },
  "keywords": [
    "tab",
    "carousel",
    "loadmore",
    "refresh",
    "tab-carousel",
    "vue",
    "vue-tab-carousel"
  ],
  "main": "./lib/vueTabCarousel.umd.js",
  "private": false,
  "homepage": "https://github.com/magicianShiro/vue-tab-carousel#readme",
  "author": "magicianShiro <navi@movom.cc>",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js",
    "build:lib": "vue-cli-service build --target lib --name vueTabCarousel --dest lib src/index.js",
    "build": "vue-cli-service build ./example/main.js",
    "lint": "vue-cli-service lint",
    "patch": "npm version patch && git push origin master",
    "minor": "npm version minor && git push origin master && git push origin --tags",
    "major": "npm version major && git push origin master && git push origin --tags"
  },
  "dependencies": {
    "alloytouch": "^0.2.5",
    "vue": "^2.5.17",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^1.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.3",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
